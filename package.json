{
  "name": "tradefed-frontend",
  "version": "1.9.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-calendar": "^3.1.4",
    "@types/react-dom": "^16.9.0",
    "@types/react-instantsearch-dom": "^6.8.0",
    "algoliasearch": "^4.8.5",
    "apexcharts": "^3.27.1",
    "apollo-boost": "^0.4.9",
    "axios": "^0.19.2",
    "dayjs": "^1.10.4",
    "emotion-theming": "^10.0.27",
    "formik": "^2.1.4",
    "framer-motion": "^2.0.0-beta.71",
    "graphql": "^15.0.0",
    "lodash": "^4.17.15",
    "mixpanel-browser": "^2.41.0",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.9",
    "react-calendar": "^3.4.0",
    "react-dates": "^21.8.0",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-flexbox-grid": "^2.1.2",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.10.3",
    "react-intersection-observer": "^8.26.2",
    "react-responsive": "^8.0.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-table": "^7.1.0",
    "styled-system": "^5.1.5",
    "typescript": "~3.7.2",
    "use-places-autocomplete": "^1.7.1",
    "use-react-router-breadcrumbs": "^1.0.4",
    "yup": "^0.28.5"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "generate": "graphql-codegen --config codegen.yml",
    "start": "set -o allexport; source .env; set +o allexport && yarn generate && react-scripts start",
    "test": "react-scripts test",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|scss|ts|tsx)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/react-components": "^3.1.5",
    "@apollo/react-hoc": "^3.1.5",
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/typescript-react-apollo": "^1.13.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/googlemaps": "^3.43.1",
    "@types/lodash": "^4.14.151",
    "@types/mixpanel-browser": "^2.35.6",
    "@types/react-dates": "^21.8.2",
    "@types/react-helmet": "^6.0.0",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-table": "^7.0.17",
    "@types/yup": "^0.28.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7"
  },
  "release": {
    "branches": [
      "develop"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx,json,css}": [
      "yarn prettier --write",
      "yarn lint:fix"
    ]
  }
}
